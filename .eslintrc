{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react-proptypes": "off"
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "arrow-parens": [
      "error",
      "always"
    ],
    "func-names": [
      "error",
      "never"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "error"
        ]
      }
    ],
    "react/no-danger": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/click-events-have-key-events": 0
  },
  "globals": {
    "window": true,
    "document": true
  }
}